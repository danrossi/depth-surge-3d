[flake8]
max-line-length = 88
extend-ignore = E203, E266, E501, W503
exclude =
    .git,
    __pycache__,
    .venv,
    venv,
    build,
    dist,
    *.egg-info,
    .pytest_cache,
    htmlcov,
    video_depth_anything_repo
per-file-ignores =
    __init__.py:F401,F403,F405,E402
max-complexity = 10
